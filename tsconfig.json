{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "declaration": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "~*": ["./*"]
    }
  },
  "exclude": ["node_modules", "server", "results"],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"]
}
